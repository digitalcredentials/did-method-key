{
  "name": "@digitalcredentials/did-method-key",
  "version": "2.0.4-0",
  "description": "A did:key method resolver.",
  "homepage": "http://github.com/digitalcredentials/did-method-key",
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalcredentials/did-method-key"
  },
  "license": "BSD-3-Clause",
  "files": [
    "dist",
    "lib",
    "rollup.config.js",
    "build-dist.sh",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@digitalcredentials/did-io": "^1.0.2",
    "@digitalcredentials/x25519-key-agreement-key-2020": "^2.0.2"
  },
  "devDependencies": {

    "@digitalbazaar/bls12-381-multikey": "^1.1.1",
    "@digitalbazaar/ecdsa-multikey": "^1.1.1",
    "@digitalbazaar/ed25519-verification-key-2018": "^4.0.0",
    "@digitalbazaar/ed25519-verification-key-2020": "^4.0.0",
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-digitalbazaar": "^4.2.0",
    "eslint-plugin-jsdoc": "^40.1.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "karma": "^6.3.20",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "webpack": "^5.73.0",

    "rimraf": "^3.0.2",
    "rollup": "^2.47.0"

  },
  "c8": {
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "browser": {
    "buffer": false,
    "crypto": false,
    "util": false
  },
  "keywords": [
    "Decentralized",
    "Identifier",
    "DID"
  ],
  "scripts": {
    "rollup": "rollup -c rollup.config.js",
    "build": "npm run clear && npm run rollup && ./build-dist.sh",
    "clear": "rimraf dist/ && mkdir dist",
    "prepare": "npm run build",
    "prepack": "npm run build",
    "rebuild": "npm run clear && npm run build",

    "test": "npm run test-node && npm run test-karma",
    "test-node": "cross-env NODE_ENV=test mocha -r esm --preserve-symlinks -t 10000 test/*.spec.js",
    "test-karma": "karma start karma.conf.js",
    "coverage": "cross-env NODE_ENV=test c8 npm run test-node",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly --reporter=text-summary --reporter=text npm run test-node",
    "coverage-report": "c8 report",
    "lint": "eslint ."
  }
}
